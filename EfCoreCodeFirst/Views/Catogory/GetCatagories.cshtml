@model List<Catagory>

Add to new Catagory pls<a asp-action="CreateCatogory">Cilick</a>
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>CatagoryName</th>
        <th>Description</th>
        <th>Modified</th>
        <th>Delete</th>
    </tr>
    @foreach (Catagory item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.CatagoryName</td>
            <td>@item.Description</td>
            <td> <a asp-action="UptadeCatagory" asp-all-route-id="@item.ID" class="btn btn-warning">Modified</a>  </td>
            <td> <a asp-action="DeleteCatagory" asp-all-route-id="@item.ID" class="btn btn-danger">Delete</a>  </td>

        </tr>
    }



</table>